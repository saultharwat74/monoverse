{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src/",
    "paths": {
      "assets": [
        "./assets"
      ],
      "components": [
        "./components"
      ],
      "screens": [
        "./screens"
      ],
      "types": [
        "./types"
      ],
      "routers": [
        "./routers"
      ],
      "state": [
        "./state"
      ],
      "constants": [
        "./constants"
      ],
      "hooks": [
        "./hooks"
      ],
      "emitDecoratorMetadata": [
        "true"
      ],
      "experimentalDecorators": [
        "true"
      ],
      "allowJs": [
        "true"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}